body {
  counter-reset: steps-counter 0 section 0;
 
}

.ek-section {
	margin-bottom: 2em;
}

.ek-section-block {
  display: block; 
  margin: 1em 0; 
  position: relative
}

.ek-section-block .option--label:before {
    counter-increment: section;  
    content: counter(section);   
    width: 28px;
    height: 28px;
    border-radius: 5px;
    text-align: center;
    line-height: 28px;
    background: #ECECEC;
    color: #000;
    position: absolute;
    top: 0em;
    left: 0;
    font-weight: 400;
    text-transform: uppercase;
    font-size: 0.8823529412em;
    letter-spacing: .1em;
}

.ek-section-block label.option--label span {
    display: block;
    padding-left: 42px;
  	line-height: 28px;
  	text-align: left;
  	background-color: transparent;
  	border-bottom: none;
}

.ek-section input, .ek-section textarea, .ek-section select {
	border-color: #cccccc;
}

.ek-d-none {display: none!important;}

form.product-form.product-form-product-customizable-template .product-form__btn-container  {
	padding-top: 1em;
}

header.is-moved-by-drawer.site-header.logo--left {
    max-height: 100px;
    overflow: hidden;
}

.lower_photos {
  position: absolute;
  left: 0;
  right: 0;
  display: none;
  margin-top: 3rem;
  
  &:after {
  	content: "";
    position: absolute;
    left: 15px;
    right: 15px;
    height: 1px;
    background: #ECECEC;
    top: 45px;
  }
  
  
  & .ek-steps {
  		display: inline-block;
    	width: 30%;
    	position: relative;
    
    &:before {
      text-transform: uppercase;
      font-weight: bold;
      font-size: 13px;
      line-height: 28px;
      letter-spacing: 0.5px;
      counter-increment: steps-counter;
      content: "Step " counter(steps-counter);
    }
    
    &:after {
    	content: "";
        position: absolute;
        width: 0;
        height: 0;
        border-left: 10px solid transparent;
        border-right: 10px solid transparent;
        border-bottom: 10px solid #ECECEC;
        left: 0;
        right: 0;
        margin: 0 auto;
        top: 35px;
    }
    
    & span {
    	font-size: 14px;
    	font-weight: 300;
      	-webkit-font-smoothing: antialiased;
      	font-smoothing: antialiased !important;
    }
    
    & .icon {
    	height: 50px;
        width: 50px;
        display: block;
        margin: 25px auto 0;
      	background-position: 50%;
    	background-repeat: no-repeat;
    }
    
    & .icon-trim-color {
      	background-size: contain;
    	background-image: url('data:image/png;base64,');
    }
    
    & .icon-text {
      	background-size: 60%;
    	background-image: url('data:image/png;base64,');
    }
    
    & .icon-text-color {
      	background-size: 90%;
    	background-image: url('data:image/png;base64,');
    }
  
    
    & img {
    	display: block;
    	margin: 50px auto 10px;
    }
  }	
}

.grid__item.ek-product-single {
	max-width: 100%;
  	max-width: 600px;
  	margin: 0 auto;
